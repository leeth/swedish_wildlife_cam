{
  "type": "container",
  "containerProperties": {
    "image": "${DetectorImageUri}",
    "vcpus": 2,
    "memory": 4096,
    "command": [],
    "environment": [
      {
        "Name": "PYTHONPATH",
        "Value": "/app/src"
      }
    ],
    "jobRoleArn": "${BatchJobRoleArn}",
    "executionRoleArn": "${BatchExecRoleArn}",
    "mountPoints": [],
    "volumes": [],
    "ulimits": [],
    "resourceRequirements": []
  },
  "retryStrategy": {
    "attempts": 1
  },
  "timeout": {
    "attemptDurationSeconds": 3600
  },
  "platformCapabilities": ["EC2"],
  "tags": {
    "Project": "WildlifePipeline",
    "Stage": "Detection"
  }
}

